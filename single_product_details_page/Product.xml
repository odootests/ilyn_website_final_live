<?xml version="1.0"?>
<t name="Product" t-name="website_sale.product">
    <t t-call="website.layout">
        <t t-set="additional_title" t-value="product.name"/>
        <style>
            .sliderImgPre, .sliderImgNext{
                      top:37% !important;
                  }
                  
            
                  
            .carousel-control:hover, .carousel-control:focus {
                    outline: 0;
                    color: #ffffff !important;
                    text-decoration: none !important;
                    opacity: 0.5;
                    filter: alpha(opacity=60);
                }
          
            .first_row{
                background-color: #f0f0f0;
                width: 100%;
            }
            ol.breadcrumb{
                background-color: transparent;
                padding: 25px 0px;
                margin-bottom: 0;
            }
            ol.breadcrumb li a, ol.breadcrumb li.active{
                font-size: 12px;
                color: #656565;
                text-transform: uppercase;
                text-decoration:none;
            }
            
            .product_name_heading_spv{
                /*font-size:24px;*/
                color: #252525;
                font-weight: bold;
                text-transform: uppercase;
                margin-top:-5px !important
            }
            #product_details{
                padding-left: 36px;
            }
            .product_desc_pt_spv{
                line-height: 24px;
                color: #797979;
            }
            #add_to_cart{
                display: inline-block !important;
                width: 196px;
                background: #c1b497;
                font-size: 14px;
                line-height: 1.8;
                color:white;
                text-transform: uppercase;
                padding:10px 24px;
                border-radius: 0;
                -webkit-transition: all 0.3s ease-in-out;
                -moz-transition: all 0.3s ease-in-out;
                -ms-transition: all 0.3s ease-in-out;
                -o-transition: all 0.3s ease-in-out;
                transition: all 0.3s ease-in-out;
            }
            #add_to_cart:hover{
                /* background: #cdc3ab; */ 
                color:#fff !important;
            } 
            
            
            
            
            
            
            .alert{ 
                border-radius 0;
            }
            .js_variant_change{
                margin-right:0 !important;
            }
            .control-label{
                margin: 0 10px !important;
                font-size: 12px;
            }
            input[type="radio"], input[type="checkbox"] {
                margin: 2px 0 0;
                -webkit-transition: all 0.25s ease-in-out;
                -moz-transition: all 0.25s ease-in-out;
                -ms-transition: all 0.25s ease-in-out;
                -o-transition: all 0.25s ease-in-out;
                transition: all 0.25s ease-in-out;
                
            }
            input[type="radio"]:focus{
                -ms-transform: scale(1.3);
                -webkit-transform: scale(1.3); 
                transform: scale(1.3);
            }
            .product_detail_img{
                margin-left:0 !important;
            }
            
           
            .size_guide_div{
               padding: 20px 0px 35px 0px ;
              /* border: 2px solid #c1b497; */
                
                
            }
            
            /* size div alignment */
            .js_add_cart_variants{
             margin-bottom: 26px !important;
            }
           
           
          .o_alternative_product{
                display: block;
               /*  width: 100% !important; */
                max-height: 100%;
                margin-bottom: -10% !important;
            } 
            .thumbnail {
               display: block;
               padding: 4px;
               margin-bottom: 20px;
               line-height: 1.42857143;
               background-color: #ffffff;
               border:none !important;
               text-align:center !important;
           }
             
            .hr{
                height:2px;
                background-color:#cccccc;
                width: 99% !important;
            }
            
            .carousel .carousel-control.right {
                right: 75px;
            }
            #o-carousel-product .carousel-indicators li {
                width: 110px !important;
            }
            
            
            hr{
               padding: 4px 0;
               width: 100%;
            }
        
