<data inherit_id="website.footer_custom" customize_show="True" name="Automatic Footer">
    <xpath expr="//div[@id='footer']" position="replace">
        <style>
            #clothing_list{
                padding-left:0px !important;
                padding-right:0px !important;
            }
            #clothing_listSub{
                padding-left:0px !important;
                padding-right:0px !important;
            }
            footer #footer .footer-link h5 a{
              color: #5f5f5f !important;
              
            }
            a h5:hover{color:#c1b497 !important}
            
            
            .disp-cen-add-heading{
                line-height: 16px;
                letter-spacing: 1.5px;
                font-size:11px;
            }
            
            
            footer #footer .footer-link ul li {
                color: #acacac;
                font-size: 12px;
                line-height: 20px;
            }
            footer #footer .footer-link ul li a {
                color: #acacac !important;
                font-size: 12px;
                line-height: 20px;
                text-decoration: none !important;
                -webkit-transition: all 0.3s ease-in-out;
                -moz-transition: all 0.3s ease-in-out;
                -ms-transition: all 0.3s ease-in-out;
                -o-transition: all 0.3s ease-in-out;
                transition: all 0.3s ease-in-out;
            }
            footer #footer .footer-link ul li a:hover{
                color: #c1b497 !important;
            }
            
            .anc_l1, .anc_tdn{
                color: #5f5f5f;
                -webkit-transition: all 0.3s ease-in-out;
                -moz-transition: all 0.3s ease-in-out;
                -ms-transition: all 0.3s ease-in-out;
                -o-transition: all 0.3s ease-in-out;
                transition: all 0.3s ease-in-out;
            }
            .anc_l1:hover, .anc_tdn:hover, .anc_l1:focus, .anc_l1:active{
                color: #c1b497 !important;
                text-decoration: none !important;
            }
            a.anc_tdn:hover,a.anc_tdn:focus,a.anc_tdn:active{
                color:#c1b497 !important;
                text-decoration: none;
            }
           
            .anc_fc_gray{
                color: #acacac;
            }
            .footer_msg_p{
                color: #acacac;
                font-size: 12px;
            }
            .perfume_heading{
                letter-spacing: 1px;
                color: #5f5f5f;
                text-transform: uppercase;
                margin-bottom: 13px;
                margin-top: 22px;
                font-size: 14px;
                font-weight: bold;
            }
            .pad_lrz{ 
                padding-left:0;
                padding-right:0
            }
            #subscription_form_submit{
                background: transparent;
                color: white;
                border-radius: 0;
                text-transform: uppercase;
                font-size:12px;
                border: 2px solid #c1b497 !important;
                
            }
            .o_website_form_input{
                border-radius: 0;
            }
            
            textarea:focus,input[type="text"]:focus,input[type="password"]:focus,input[type="datetime"]:focus,input[type="datetime-local"]:focus,input[type="date"]:focus,input[type="month"]:focus,input[type="time"]:focus,input[type="week"]:focus,input[type="number"]:focus,input[type="email"]:focus,input[type="url"]:focus,input[type="search"]:focus,input[type="tel"]:focus,input[type="color"]:focus,.uneditable-input:focus {   
                outline: none;
                box-shadow:none !important;
                border: 2px solid #c1b497 !important;
                
            }
            
            #footer_subscription_customer_name, #footer_subscription_customer_phone{
                background: #1d1a18;
                color: #c1b497;
                border: none;
                font-size: 11px; 
            }
            
            #your_cell_text_label, #your_cell_cc_label{
                background-color: #1d1a18;
            }
            
            .input-group-addon{
                border-radius:0;
                border:none;
                color: #c1b497;
                font-size:12px;
            }

            input#footer_subscription_customer_phone::-webkit-outer-spin-button,input#footer_subscription_customer_phone::-webkit-inner-spin-button {
                -webkit-appearance: none;
                -ms-appearance: none;
                -o-appearance: none;
                margin: 0;
            }
            input#footer_subscription_customer_phone{
               -moz-appearance:textfield;
            }
           
        
            </style>
